$TTL                            604800

@               IN      SOA     ns1.{{ external_dns_domain }}. admin.{{ external_dns_domain }}. (
                                1               ; Serial
                                604800          ; Refresh
                                86400           ; Retry
                                2419200         ; Expire
                                604800 )        ; Negative Cache TTL

; name servers - NS records
@               IN      NS      ns1.{{ external_dns_domain }}.

; name servers - A records
ns1             IN      A       {{ external_ip_address__enc }}

; A records
vpn             IN      A       {{ external_ip_address__enc }}
