;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     duke.lan. admin.duke.lan. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
; name servers - NS records
        IN      NS      192.168.1.53.

; name servers - A records
nameserver.duke.lan.        IN      A       192.168.1.53


; 192.168.1.0/24 - A records
gateway.duke.lan.           IN      A       192.168.1.1
pve.duke.lan.               IN      A       192.168.1.50
pve-M910q-01.duke.lan.      IN      A       192.168.1.40
zabbix.duke.lan.            IN      A       192.168.1.51
docker.duke.lan.            IN      A       192.168.1.52
